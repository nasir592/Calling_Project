{
  "kind": "collectionType",
  "collectionName": "expert_profiles",
  "info": {
    "singularName": "expert-profile",
    "pluralName": "expert-profiles",
    "displayName": "Expert_Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "specialization": {
      "type": "string",
      "required": true
    },
    "descriptions": {
      "type": "text",
      "maxLength": 120
    },
    "totalMins": {
      "type": "biginteger",
      "default": "0"
    },
    "languages": {
      "type": "text"
    },
    "tagline": {
      "type": "string"
    },
    "handler": {
      "type": "string",
      "unique": true
    },
    "schedule": {
      "type": "component",
      "repeatable": true,
      "component": "schedule.availability"
    },
    "rates": {
      "type": "component",
      "repeatable": false,
      "component": "rate.rates"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::public-user.public-user",
      "inversedBy": "expert"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "expert"
    },
    "averageRating": {
      "type": "decimal"
    },
    "reviewCount": {
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "expert"
    },
    "firebaseTokens": {
      "type": "text"
    }
  }
}
