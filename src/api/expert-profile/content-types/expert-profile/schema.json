{
  "kind": "collectionType",
  "collectionName": "expert_profiles",
  "info": {
    "singularName": "expert-profile",
    "pluralName": "expert-profiles",
    "displayName": "Expert_Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "specialization": {
      "type": "string",
      "required": true
    },
    "experience": {
      "type": "integer"
    },
    "bio": {
      "type": "text",
      "maxLength": 120
    },
    "availableFrom": {
      "type": "time"
    },
    "availableTo": {
      "type": "time"
    },
    "daysAvailable": {
      "type": "enumeration",
      "enum": [
        "Monday ",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Saturday",
        "Sunday"
      ]
    },
    "videoCallRate": {
      "type": "integer",
      "default": 0
    },
    "voiceCallRate": {
      "type": "integer",
      "default": 0
    },
    "totalMins": {
      "type": "biginteger",
      "default": "0"
    }
  }
}
