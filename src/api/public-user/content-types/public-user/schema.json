{
  "kind": "collectionType",
  "collectionName": "public_users",
  "info": {
    "singularName": "public-user",
    "pluralName": "public-users",
    "displayName": "Public_Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "General",
        "Expert"
      ],
      "default": "General",
      "required": true
    },
    "profilePic": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "mobile": {
      "type": "biginteger",
      "required": true,
      "unique": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "user"
    },
    "calls": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::call.call",
      "mappedBy": "receiver"
    },
    "expert_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::expert-profile.expert-profile"
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "user"
    },
    "wallet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wallet.wallet"
    }
  }
}
